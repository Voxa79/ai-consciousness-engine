{
  "name": "consciousness-mobile",
  "version": "1.0.0",
  "description": "Consciousness Engine Mobile App",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace ConsciousnessMobile.xcworkspace -scheme ConsciousnessMobile -configuration Release -destination generic/platform=iOS -archivePath ConsciousnessMobile.xcarchive archive",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "bundle:ios": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle --assets-dest ios"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-vector-icons": "^10.0.0",
    "react-navigation": "^6.0.0",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "react-native-screens": "^3.27.0",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-gesture-handler": "^2.13.4",
    "react-native-reanimated": "^3.5.4",
    "react-native-svg": "^13.14.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-async-storage": "@react-native-async-storage/async-storage",
    "react-native-keychain": "^8.1.3",
    "react-native-biometrics": "^3.0.1",
    "react-native-push-notification": "^8.1.1",
    "react-native-voice": "^3.2.4",
    "react-native-tts": "^4.1.0",
    "react-native-sound": "^0.11.2",
    "react-native-camera": "^4.2.1",
    "react-native-image-picker": "^7.0.3",
    "react-native-document-picker": "^9.1.1",
    "react-native-share": "^10.0.2",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-get-random-values": "^1.9.0",
    "react-native-uuid": "^2.0.1",
    "axios": "^1.6.0",
    "socket.io-client": "^4.7.4",
    "zustand": "^4.4.7",
    "react-hook-form": "^7.47.0",
    "react-native-hook-form": "^1.0.0",
    "date-fns": "^2.30.0",
    "react-native-date-picker": "^4.3.3",
    "react-native-modal": "^13.0.1",
    "react-native-toast-message": "^2.1.7",
    "react-native-loading-spinner-overlay": "^3.0.1",
    "react-native-progress": "^5.0.1",
    "react-native-chart-kit": "^6.12.0",
    "react-native-animatable": "^1.3.3",
    "react-native-haptic-feedback": "^2.2.0",
    "react-native-device-info": "^10.11.0",
    "react-native-network-info": "^5.2.1",
    "react-native-permissions": "^4.0.0",
    "react-native-background-task": "^0.2.1",
    "react-native-background-job": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4",
    "@types/react-native": "^0.72.6",
    "@types/react-native-vector-icons": "^6.4.18",
    "detox": "^20.13.5",
    "flipper-plugin-react-native-performance": "^0.3.0"
  },
  "engines": {
    "node": ">=16"
  },
  "jest": {
    "preset": "react-native"
  }
}
