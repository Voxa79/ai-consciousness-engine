    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "clean": "rimraf dist .netlify",
    "netlify:dev": "netlify dev",
    "netlify:build": "netlify build",
    "netlify:deploy": "netlify deploy",
    "netlify:deploy:prod": "netlify deploy --prod",
