    "build:functions": "netlify-lambda build netlify/functions-src",
    "optimize": "npm run optimize:images && npm run optimize:assets",
    "optimize:images": "imagemin dist/assets/images/* --out-dir=dist/assets/images --plugin=imagemin-webp",
